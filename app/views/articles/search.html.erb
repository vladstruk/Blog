<h4> Articles </h4>
<table class="table table-striped">
  <th> Id </th>
  <th> Title </th>
  <th> Content </th>
  <% @articles.each do |article|%>
    <tr>
      <td> <%= article.id %> </td>
      <td> <%= link_to article.title, article_path(article) %> </td>
      <td> <%= article.content%> </td>
    <tr>
  <% end %>
</table>