<h4> Create new subsription </h4>
<%= form_for @subscription, url: admin_subscriptions_path do |f| %>
  <p>
  	<%= f.label :name %> </br>
  	<%= f.text_field :name%>
  </p>
  <p>
  	<%= f.label :period %> </br>
  	<%= f.text_field :period%>
  </p>
  <p>
  	<%= f.label :price %> </br>
  	<%= f.text_field :price%>
  </p>
  <%= f.submit class: "btn btn-warning" %>
<% end %>